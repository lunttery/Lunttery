<%= link_to "新增", new_shop_path %>

<table class="table">
  <thead>
    <th>名稱</th>
    <th>地址</th>
    <th>評分</th>
    <th>經度</th>
    <th>緯度</th>
    <th></th>
  </thead>
  <tbody>
    <% @shops.each do |shop| %>
      <tr>
        <td><%= shop[:name] %></td>
        <td><%= shop[:address] %></td>
        <td><%= shop[:rate] %></td>
        <td><%= shop[:lng] %></td>
        <td><%= shop[:lat] %></td>
        <td><%= link_to "新增", shops_path(shop: shop), method: :post %></td>
      </tr>
    <% end %>
  </tbody>
</table>
